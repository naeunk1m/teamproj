<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>movie</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AdminController</servlet-name>
    <servlet-class>admin.AdminController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminController</servlet-name>
    <url-pattern>*.am</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MemberFrontController</servlet-name>
    <servlet-class>com.cinemaw.member.action.MemberFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MemberFrontController</servlet-name>
    <url-pattern>*.me</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MovieFrontController</servlet-name>
    <servlet-class>com.cinemaw.movie.action.MovieFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MovieFrontController</servlet-name>
    <url-pattern>*.mo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PointFrontController</servlet-name>
    <servlet-class>com.cinemaw.point.action.PointFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PointFrontController</servlet-name>
    <url-pattern>*.po</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReviewFrontContoller</servlet-name>
    <servlet-class>com.cinemaw.review.action.ReviewFrontContoller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReviewFrontContoller</servlet-name>
    <url-pattern>*.re</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReserveFrontController</servlet-name>
    <servlet-class>com.cinemaw.reserve.action.ReserveFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReserveFrontController</servlet-name>
    <url-pattern>*.res</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReserveFrontController1</servlet-name>
    <servlet-class>com.cinemaw.rsv.action.ReserveFrontController1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReserveFrontController1</servlet-name>
    <url-pattern>*.rl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BoardFrontController</servlet-name>
    <servlet-class>com.cinemaw.board.action.BoardFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardFrontController</servlet-name>
    <url-pattern>*.bo</url-pattern>
  </servlet-mapping>
</web-app>